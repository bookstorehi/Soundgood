<Application
    x:Class="Soundgood.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Soundgood">
    <Application.Resources>
        <CornerRadius x:Key="GridViewItemCornerRadius">5</CornerRadius>
        <BitmapImage x:Key="DefaultImage" UriSource="/image_icon.png" />
        <BitmapImage x:Key="PlayIcon" UriSource="/play_icon.png" />
        <BitmapImage x:Key="PauseIcon" UriSource="/pause_icon.png" />
    </Application.Resources>
</Application>
